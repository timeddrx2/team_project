.xiugai-diandan {
  display: flex;
  flex-direction: column;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: right;
  background-size:20% 30%;
}

.name-top {
  position: relative;
  margin-top: 10rpx;
  margin-bottom: 50rpx;
  left: 20%;
  top: 50rpx;
  color: black;
  font-size: 38rpx;
}
.name {
  position: relative;
  left: 20%;
  color: black;
  font-size: 38rpx;
}
#peisong-address {
  position: relative;
  left: 20%;
  color: black;
  font-size: 38rpx;
}
.lou-men {
  position: relative;
  margin-top: 20rpx;
  margin-left: 25%;
  color: black;
  font-size: 38rpx;
}
.address{
  display: flex;
  flex-direction: row;

}
#add-lou {
  border: solid black 3rpx;
  border-radius: 5px;
  height: 25px;
  width: 30%;
  margin-left: 10rpx;
  position: relative;
  margin-top: 10rpx;
  font-size: 38rpx;
  text-align: center;
}
#add-hao {
  border: solid black 3rpx;
  border-radius: 5px;
  height: 25px;
  width: 30%;
  position: relative;
  margin-top: 20rpx;
  font-size: 38rpx;
  margin-left: 10rpx;
  text-align: center;
}
.input {
  border: solid black 3rpx;
  border-radius: 5px;
  height: 25px;
  width: 50%;
  position: relative;
  margin-top: 10rpx;
  left: 25%;
  font-size: 38rpx;
  text-align: center;
}
#require {
  border: solid black 3rpx;
  border-radius: 5px;
  position: relative;
  margin-top: 10rpx;
  height: 125rpx;
  width: 50%;
  left: 25%;
  font-size: 38rpx;
}

.btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 60rpx;
  background: #FFD700;
  color: black;
  height: 25px;
  width: 50%;
  font-size: 38rpx;
}
.btn::after {
  border: 0;
}
.btn_hover {
  background-color: #f0ca00;
}

.line {
  width: 60%;
  border: solid #e0e3da 2rpx;
  position: relative;
  left: 20%;
