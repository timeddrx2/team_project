<!--order_detail.wxml-->
<view class="background">
  <view class="view_detail">
    <view class="view_detail_text">
      <text>地点：\n</text>
      <text>配送费：\n</text>
      <text>配送地址：\n</text>
      <text>预计配送时间：\n</text>
      <text>接单时间：\n</text>
      <text>\n\n</text>
    </view>
  </view>
  <view class="view_remarks">
    <view class=" view_remarks_text ">
      <text>用户备注：\n</text>
      <text>\n</text>
      <text>配送员备注：\n</text>
      <text>\n</text>
    </view>
  </view>
  <view class="view_number ">
    <view class="view_number_text ">
      <text>用户联系电话：\n</text>
      <text>\n</text>
      <text>配送员联系电话：\n</text>
      <text>\n</text>
    </view>
  </view>

</view>